<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="/node_modules/bootstrap/dist/css/bootstrap.min.css" />
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">Project</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse navbar-expand-lg" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" aria-current="page" asp-action="Index" asp-controller="Home">Home</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Tables
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" aria-current="page" asp-action="Index" asp-controller="Customer">Customer</a>
                            <a class="dropdown-item" aria-current="page" asp-action="Index" asp-controller="Blog">Blog</a>
                            <a class="dropdown-item" aria-current="page" asp-action="Index" asp-controller="Product">Product</a>
                        </div>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown2" role="button" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Modules
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown2">
                            <a class="dropdown-item" aria-current="page" asp-action="Index" asp-controller="Session">Session</a>
                            <a class="dropdown-item" aria-current="page" asp-action="Index" asp-controller="Cookie">Cookie</a>
                            <a class="dropdown-item" aria-current="page" asp-action="Index" asp-controller="Folder">Folder</a>
                            <a class="dropdown-item" aria-current="page" asp-action="Index" asp-controller="File">File</a>
                            <a class="dropdown-item" aria-current="page" asp-action="ErrorTest" asp-controller="Error">Hata</a>
                            <a class="dropdown-item" aria-current="page" asp-action="StatusTest" asp-controller="Status">Status</a>
                            <a class="dropdown-item" aria-current="page" asp-action="Index" asp-controller="TagHelper">Tag Helper</a>
                            <a class="dropdown-item" aria-current="page" asp-action="Index" asp-controller="ViewComponentExample">View Component</a>
                            <a class="dropdown-item" aria-current="page" asp-action="Index" asp-controller="GMap">Maps Place</a>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container-fluid p-4">
        @RenderBody()
    </div>

    <footer class="bg-dark p-2 text-center text-white fixed-bottom">
        Her Hakkı Saklıdır
    </footer>

    <script src="/node_modules/@@popperjs/core/dist/umd/popper.min.js"></script>
    <script src="/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>

    @RenderSection("Scripts",false)
</body>
</html>


